/**
 * 16-bit register:
 * If load[t] == 1 then out[t+1] = in[t]
 * else out does not change
 */

CHIP Register {
    IN in[16], load;
    OUT out[16];

    PARTS:
    // Put your code here:
    Mux(a=out[0], b=in[0], sel=load, out=Mux0);
    Mux(a=out[0], b=in[0], sel=load, out=Mux0);
}
